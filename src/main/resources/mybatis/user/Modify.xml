<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.portfolio.sellf.domain.user.modify.mapper.JoinMapper">

  <!--회원정보 수정-->
  <update id="updateUser" parameterType="com.portfolio.sellf.domain.user.join.vo.UserVo">
    UPDATE TB_USER SET USER_NAME = #{USER_NAME}, USER_PASSWORD = #{USER_PASSWORD}, USER_EMAIL = #{USER_EMAIL} WHERE USER_ID = #{USER_ID}
  </update>
</mapper>

