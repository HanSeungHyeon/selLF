<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.portfolio.sellf.domain.join.mapper.JoinMapper">

  <!-- 회원가입 -->
  <insert id="joinUser" useGeneratedKeys="true" keyProperty="accountVo.userNo">
  INSERT INTO TB_USER (
    USER_NO
    ,USER_ID
    ,USER_PASSWORD
    ,USER_NAME
    ,USER_NICKNAME
    ,USER_PROFILE_IMAGE
    ,USER_TEMPORARY_YN
    ,USER_SHARE_CODE
  ) VALUES (
    #{accountVo.userNo}
    ,#{accountVo.userId}
    ,#{accountVo.userPassword}
    ,#{accountVo.userId}
    ,#{accountVo.userNickname}
    ,#{accountVo.userProfileImage}
    ,#{accountVo.userTemporaryYn}
    ,#{accountVo.userShareCode}
  )
  </insert>

</mapper>

